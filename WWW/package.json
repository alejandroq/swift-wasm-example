  "dependencies": {
    "@wasmer/wasi": "^0.9.1",
    "@wasmer/wasmfs": "^0.9.1",
    "javascript-kit-swift": "^0.5.0",
    "wasm-opt": "^1.1.1"
  },
  "devDependencies": {
    "@webassemblyjs/wasm-strip": "^1.8.1",
